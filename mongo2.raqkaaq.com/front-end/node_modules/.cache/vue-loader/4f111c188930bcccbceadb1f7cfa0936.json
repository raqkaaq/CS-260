 = await axios.get(\"/api/tickets\");\n        this.tickets = response.data.tickets;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteTicket(ticket) {\n      try {\n        await axios.delete(\"/api/tickets/\" + ticket.id);\n        this.getTickets();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n"]}]}